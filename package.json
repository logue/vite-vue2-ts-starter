{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-ts-starter",
  "description": "Vue2 TypeScript Startar project for Vite.",
  "version": "1.0.12",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vue2-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vue2-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vue2-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.14.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest run --coverage",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/vue2-helpers": "^2.1.3",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.3.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.1",
    "stylelint": "^15.1.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-checker": "^0.5.5",
    "vitest": "^0.28.5",
    "vue-eslint-parser": "^9.1.0",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,toml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
